{
    "name": "faryshta/yii2-enum",
    "description": "Enum classes for Yii2 models and forms",
    "keywords": [
        "yii2",
        "enum"
    ],
    "homepage": "http://www.yiiframework.com/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Angel (Faryshta) Guevara",
            "email": "angeldelcaos@gmail.com",
            "role": "Founder and project lead"
        }
    ],
    "support": {
        "issues": "https://github.com/Faryshta/yii2-enum/issues?state=open",
        "source": "https://github.com/Faryshta/yii2-enum"
    },
    "minimum-stability": "dev",
    "replace": {
        "yiisoft/yii2": "self.version"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "cebe/indent": "*"
    },
    "suggest": {
        "yiisoft/yii2-coding-standards": "you can use this package to check for code style issues when contributing to yii"
    },
    "autoload": {
        "psr-4": {
            "faryshta\\": ""
        }
    }
}
